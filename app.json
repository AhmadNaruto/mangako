{
  "expo": {
    "name": "MangaKo",
    "slug": "githubbuild",
    "scheme": "MangaKo",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/ramen_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ramen_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1D2434"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ramen_icon.png",
        "backgroundColor": "#1D2434"
      },
      "package": "com.husph.MangaKo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1a0e699f-d178-4bfe-8d97-5e463fc4fae8"
      }
    }
  }
}
