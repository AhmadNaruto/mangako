{
  "expo": {
    "name": "MangaKo",
    "slug": "MangaKo",
    "scheme": "MangaKo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/ramen_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ramen_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1D2434"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ramen_icon.png",
        "backgroundColor": "#1D2434"
      },
      "package": "com.husph.MangaKo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e7119cef-eb3c-4885-ae2d-8dbdbf9f5d6b"
      }
    }
  }
}
